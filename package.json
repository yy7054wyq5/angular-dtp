    "lodash": "^4.17.10",
    "muuri": "^0.7.1",
    "ng-zorro-antd": "^1.8.0",
    "ng2-dragula": "^2.1.0",
    "rxjs": "~6.2.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
