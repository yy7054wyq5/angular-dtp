{
  "name": "scm-auto-client",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "hmr": "ng serve --configuration hmr",
    "aot": "ng serve --aot --port 4200",
    "wy": "ng serve --configuration wyhmr --host localhost",
    "ie": "ng serve --prod --port 4200",
    "g-stats": "ng build --prod --base-href ./ --named-chunks --optimization --output-path dist --stats-json",
    "stats": "webpack-bundle-analyzer ./dist/stats.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "node ./src/scripts/version.js",
    "chart-theme": " node ./src/scripts/chart-theme.js",
    "prebuild": "git pull & npm install",
    "build:local": "./build/build_local.sh",
    "build:test": "./build/build_test.sh",
    "build:prod": "./build/build_prod.sh",
    "view": "gulp prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
